    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "frameworks": [
    "UIKit",
    "JavaScriptCore",
    "SystemConfiguration",
    "CoreTelephony",
    "ImageIO"
  ],
  "libraries": [
    "icucore",
    "z.1.2.5",
    "stdc++",
    "z",
    "sqlite3"
  ],
  "dependencies": {
    "APPSNetworking": [

    ]
  },
  "vendored_frameworks": "SDK/**/*.framework",
  "xcconfig": {
    "FRAMEWORK_SEARCH_PATHS": "\"${PODS_ROOT}/APPSAuthorizedLogin/SDK/ShareSDK\"/**",
    "OTHER_LDFLAGS": "\"-ObjC\""
  },
  "resources": [
    "SDK/ShareSDK/**/*.bundle",
    "SDK/script.sh"
  ],
  "source_files": [
    "APPSAuthorizedLogin/Classes/**/*",
    "SDK/ShareSDK/Support/PlatformSDK/**/*.{h,m}"
  ],
  "public_header_files": [
    "SDK/ShareSDK/Support/PlatformSDK/SinaWeiboSDK/*.h",
    "SDK/ShareSDK/Support/PlatformSDK/WeChatSDK/*.h",
    "APPSAuthorizedLogin/Classes/**/*.h"
  ],
  "vendored_libraries": "SDK/ShareSDK/Support/PlatformSDK/**/*.a"
}
